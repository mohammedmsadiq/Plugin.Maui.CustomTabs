<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CustomTabs.Sample.Pages.ProfilePage"
             Title="Profile">
    <VerticalStackLayout Padding="24" Spacing="12">
        <Label Text="Profile"
               FontSize="28"
               FontAttributes="Bold" />
        <Label Text="Another independent stack." />
        <Button Text="Push details"
                Clicked="OnPushDetailsClicked" />
    </VerticalStackLayout>
</ContentPage>
